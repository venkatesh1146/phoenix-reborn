{
  "name": "pheonix-reborn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_PUBLIC_WEALTHY_ENV=development next dev",
    "build": "next build",
    "build:dev": "NODE_ENV=production NEXT_PUBLIC_WEALTHY_ENV=development && next build",
    "build:prod": "NODE_ENV=production NEXT_PUBLIC_WEALTHY_ENV=production  && next build",
    "export": "next build && next export",
    "start": "next start",
    "start:dev": "NODE_ENV=development NEXT_PUBLIC_WEALTHY_ENV=development next start --port 80",
    "start:prod": "NODE_ENV=production NEXT_PUBLIC_WEALTHY_ENV=production next start --port 80",
    "lint": "eslint src",
    "format": "prettier src",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest --run",
    "generate": "plop",
    "storybook": "start-storybook --port 6006",
    "storybook:build": "build-storybook",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn eslint --fix",
      "yarn prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@mui/material": "^5.11.7",
    "@manojadams/metaforms": "^5.0.27",
    "@callstack/react-theme-provider": "^3.0.8",
    "@linaria/babel-preset": "^4.4.4",
    "@linaria/core": "^4.2.9",
    "@linaria/react": "^4.3.7",
    "@linaria/vite": "^4.2.10",
    "@linaria/webpack-loader": "^4.1.17",
    "@tanstack/react-query": "^4.29.7",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "framer-motion": "^10.12.16",
    "frontend-models": "github:wealthy/frontend-models",
    "graphql": "^16.6.0",
    "graphql-tag": "2.11.0",
    "humps": "^2.0.1",
    "js-cookie": "^3.0.5",
    "next": "13.1.1",
    "next-linaria": "^1.0.1-beta",
    "rc-dialog": "^9.1.0",
    "rc-select": "^7.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-transition-group": "^4.4.5",
    "sass": "^1.62.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.1.1",
    "@storybook/addon-actions": "6.5.15",
    "@storybook/addon-essentials": "6.5.15",
    "@storybook/addon-interactions": "6.5.15",
    "@storybook/addon-links": "6.5.15",
    "@storybook/builder-vite": "0.2.6",
    "@storybook/react": "6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/humps": "^2.0.2",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/react-transition-group": "^4.4.6",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "@vitejs/plugin-react": "3.0.0",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.2",
    "jsdom": "20.0.3",
    "lint-staged": "13.1.0",
    "plop": "3.1.1",
    "prettier": "2.8.1",
    "typescript": "4.9.4",
    "vite": "4.0.3",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "0.26.3"
  }
}
